	<!-- A connection to ActiveMQ for broker index BROKER_INDEX_PLACEHOLDER --> 
	<bean id="minIdUpdaterAmqConnectionFactoryBrokerBROKER_INDEX_PLACEHOLDER" class="org.apache.activemq.ActiveMQConnectionFactory">
	    <property name="brokerURL"><value>BROKER_URL_PLACEHOLDER</value></property>
	</bean>

	<!-- A cached connection to wrap the ActiveMQ connection for broker index BROKER_INDEX_PLACEHOLDER --> 
	<bean id="minIdUpdaterCachedConnectionFactoryBrokerBROKER_INDEX_PLACEHOLDER" class="org.springframework.jms.connection.CachingConnectionFactory">
	    <property name="targetConnectionFactory"><ref bean="minIdUpdaterAmqConnectionFactoryBrokerBROKER_INDEX_PLACEHOLDER" /></property>
		<property name="sessionCacheSize"><value>100</value></property>
	</bean>
	
	<!-- and this is the message listener container for broker index BROKER_INDEX_PLACEHOLDER -->
	<bean id="minIdUpdaterJmsContainerInstanceBrokerBROKER_INDEX_PLACEHOLDER" class="org.springframework.jms.listener.DefaultMessageListenerContainer">
   		<property name="connectionFactory" ref="minIdUpdaterCachedConnectionFactoryBrokerBROKER_INDEX_PLACEHOLDER"/>
   		<property name="destination" ref="minIdUpdaterDestination"/>
   		<property name="messageListener" ref="minIdUpdaterMessageListener" />
		<property name="concurrentConsumers" value="${jms.concurrentConsumers}" />
		<property name="maxConcurrentConsumers" value="${jms.maxConcurrentConsumers}" />
		<property name="sessionTransacted" value="true" />	
	</bean>
	<!-- Must have this at last line-->
	