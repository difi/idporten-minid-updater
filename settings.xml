<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <servers>
        <server>
            <id>stage</id>
            <username>${maven.stage.username}</username>
            <password>${maven.stage.password}</password>
        </server>
        <server>
            <id>eid-releases</id>
            <username>${maven.release.username}</username>
            <password>${maven.release.password}</password>
        </server>
    </servers>

</settings>
